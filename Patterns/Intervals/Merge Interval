https://leetcode.com/problems/merge-intervals/

TC - O(n logn n)
SC - O(n)

public int[][] merge(int[][] intervals) {
        // Step 1 : Sor the array based on start time
        Arrays.sort(intervals, (a,b)-> Integer.compare(a[0],b[0]));

        // Step 2 : list to process all intervals
        // in list we are processing 1D array
        List<int[]> list = new ArrayList<>();

        // Step 3 : iterate over all intervals
        for(int[] interval : intervals){
            //if list is empty than add first interval as it is
            if(list.size() == 0){
                list.add(interval);
            }
            // if list is not empty then
            else{
                // case 1 : Overlap --> Merge
                int[] prevInterval = list.get(list.size()-1);
                if(interval[0] <= prevInterval[1]){
                    prevInterval[1] = Math.max(interval[1], prevInterval[1]);
                }
                else{
                    //case 2 : No-Overlap --> add the interval as it is
                    list.add(interval);
                }
                
            }
        }
        // Step 4 : Convert list back to 2D Array & return
        return list.toArray(new int[list.size()][]);
    }
