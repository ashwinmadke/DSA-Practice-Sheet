https://leetcode.com/problems/insert-interval/

TC - O(n log n)
SC - O(n)

public int[][] insert(int[][] intervals, int[] newInterval) {
        // need list to add existing interval
        List<int[]> list = new ArrayList<>();
        
        for(int[] in : intervals){
            list.add(in);
        }
        // add new interval
        list.add(newInterval);

        // sort list based on start time
        list.sort((a,b)->Integer.compare(a[0],b[0]));

        // Merge interval logic
        List<int[]> result = new ArrayList<>();
        for(int[] interval : list){
            if(result.size() == 0){
                result.add(interval);
            }
            else{
                // overlap -> merge
                int[] prevInterval = result.get(result.size()-1);
                if(interval[0] <= prevInterval[1]){
                    prevInterval[1] = Math.max(interval[1], prevInterval[1]);
                }
                else{
                    // no overlap
                    result.add(interval);
                }
            }
        }

        // return ans by converting list into 2d array
        return result.toArray(new int[result.size()][]);
    }
